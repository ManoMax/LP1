def idosos_inicio(row):
	cont = 0
	for i in range(len(row)):
		if row[i] >= 60:
			aux = row[i]
			row[i] = row[cont]
			row[cont] = aux
			cont += 1

row = [25, 33, 67, 61, 35, 8, 12, 15, 22, 63, 75, 30, 34]
idosos_inicio(row)
assert row == [67, 61, 63, 75, 35, 8, 12, 15, 22, 25, 33, 30, 34]
